{
  "name": "@kadira/storyshots",
  "version": "2.1.0",
  "description": "StoryShots - Snapshot Testing for React Storybook.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadirahq/storyshots.git"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "bin": {
    "storyshots": "./bin/run_storyshots.js"
  },
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "NODE_ENV=development start-storybook -p 9010",
    "storyshots": "node .scripts/run_local_storyshots.js",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "peerDependencies": {
    "@kadira/storybook": "^2.17.0",
    "react": "^0.14.7 || ^15.0.0"
  },
  "devDependencies": {
    "nodemon": "1.7.x",
    "mocha": "^3.0.2",
    "chai": "^3.5.0",
    "eslint": "^3.6.0",
    "babel-eslint": "^6.1.2",
    "eslint-plugin-babel": "^3.2.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.5.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-react": "^6.3.0",
    "sinon": "^1.17.6",
    "enzyme": "^2.2.0",
    "react-addons-test-utils": "^15.3.2",
    "jsdom": "^9.5.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "@kadira/storybook": "^2.18.1",
    "git-url-parse": "^6.0.1",
    "babel-preset-react-app": "^0.2.1",
    "eslint-plugin-import": "^1.16.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "commander": "^2.9.0",
    "jest-diff": "^16.0.0",
    "jest-snapshot": "^16.0.0",
    "jsdom": "^9.5.0",
    "promptly": "^2.1.0",
    "react-test-renderer": "^15.3.1",
    "require-hijack": "^1.2.1",
    "webpack": "^1.13.3"
  }
}
